{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "jsx": "react",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    // "typeRoots": [],
    "types": ["jest"]
  }
}
